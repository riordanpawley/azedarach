{
  "name": "@effect-atom/atom",
  "version": "0.4.8",
  "description": "Reactive toolkit for Effect",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/tim-smart/effect-atom.git"
  },
  "sideEffects": [],
  "author": "Effect contributors",
  "homepage": "https://github.com/tim-smart/effect-atom",
  "peerDependencies": {
    "@effect/experimental": "^0.57.0",
    "@effect/platform": "^0.93.0",
    "@effect/rpc": "^0.72.1",
    "effect": "^3.19.0"
  },
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.js",
  "types": "./dist/dts/index.d.ts",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "types": "./dist/dts/index.d.ts",
      "import": "./dist/esm/index.js",
      "default": "./dist/cjs/index.js"
    },
    "./Atom": {
      "types": "./dist/dts/Atom.d.ts",
      "import": "./dist/esm/Atom.js",
      "default": "./dist/cjs/Atom.js"
    },
    "./AtomHttpApi": {
      "types": "./dist/dts/AtomHttpApi.d.ts",
      "import": "./dist/esm/AtomHttpApi.js",
      "default": "./dist/cjs/AtomHttpApi.js"
    },
    "./AtomRef": {
      "types": "./dist/dts/AtomRef.d.ts",
      "import": "./dist/esm/AtomRef.js",
      "default": "./dist/cjs/AtomRef.js"
    },
    "./AtomRpc": {
      "types": "./dist/dts/AtomRpc.d.ts",
      "import": "./dist/esm/AtomRpc.js",
      "default": "./dist/cjs/AtomRpc.js"
    },
    "./Hydration": {
      "types": "./dist/dts/Hydration.d.ts",
      "import": "./dist/esm/Hydration.js",
      "default": "./dist/cjs/Hydration.js"
    },
    "./Registry": {
      "types": "./dist/dts/Registry.d.ts",
      "import": "./dist/esm/Registry.js",
      "default": "./dist/cjs/Registry.js"
    },
    "./Result": {
      "types": "./dist/dts/Result.d.ts",
      "import": "./dist/esm/Result.js",
      "default": "./dist/cjs/Result.js"
    }
  },
  "typesVersions": {
    "*": {
      "Atom": [
        "./dist/dts/Atom.d.ts"
      ],
      "AtomHttpApi": [
        "./dist/dts/AtomHttpApi.d.ts"
      ],
      "AtomRef": [
        "./dist/dts/AtomRef.d.ts"
      ],
      "AtomRpc": [
        "./dist/dts/AtomRpc.d.ts"
      ],
      "Hydration": [
        "./dist/dts/Hydration.d.ts"
      ],
      "Registry": [
        "./dist/dts/Registry.d.ts"
      ],
      "Result": [
        "./dist/dts/Result.d.ts"
      ]
    }
  }
}