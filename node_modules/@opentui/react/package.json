{
  "name": "@opentui/react",
  "module": "index.js",
  "main": "index.js",
  "types": "src/index.d.ts",
  "type": "module",
  "version": "0.1.60",
  "description": "React renderer for building terminal user interfaces using OpenTUI core",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/sst/opentui",
    "directory": "packages/react"
  },
  "exports": {
    ".": {
      "types": "./src/index.d.ts",
      "import": "./index.js",
      "require": "./index.js"
    },
    "./renderer": {
      "types": "./src/reconciler/renderer.d.ts",
      "import": "./src/reconciler/renderer.js",
      "require": "./src/reconciler/renderer.js"
    },
    "./test-utils": {
      "types": "./src/test-utils.d.ts",
      "import": "./src/test-utils/test-utils.js",
      "require": "./src/test-utils/test-utils.js"
    },
    "./jsx-runtime": {
      "types": "./jsx-runtime.d.ts",
      "import": "./jsx-runtime.js",
      "require": "./jsx-runtime.js"
    },
    "./jsx-dev-runtime": {
      "types": "./jsx-dev-runtime.d.ts",
      "import": "./jsx-dev-runtime.js",
      "require": "./jsx-dev-runtime.js"
    }
  },
  "dependencies": {
    "@opentui/core": "0.1.60",
    "react-reconciler": "^0.32.0"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "@types/node": "latest",
    "@types/react": "^19.0.0",
    "@types/react-reconciler": "^0.32.0",
    "typescript": "^5"
  },
  "peerDependencies": {
    "react": ">=19.0.0"
  }
}